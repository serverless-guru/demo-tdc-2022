{
  "name": "ecs-tf",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16.14 <17",
    "yarn": ">=1.22.0 <2"
  },
  "scripts": {
    "start": "npx webpack build --config ./webpack.config.js --no-stats --watch --env watch=y",
    "build": "npx webpack build --config ./webpack.config.js --no-stats",
    "lint": "yarn run prettier:fix && yarn run ts:check && yarn run eslint:check",
    "ts:check": "npx tsc --noEmit",
    "eslint:fix": "npx eslint src --ext ts,js --fix",
    "eslint:check": "npx eslint src --ext ts,js",
    "prettier:fix": "npx prettier --write '**/*.{ts,js,json,yml,yaml,md}'",
    "preinstall": "export PUPPETEER_SKIP_DOWNLOAD=1;"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "3.127.0",
    "@aws-sdk/client-dynamodb": "3.142.0",
    "@aws-sdk/client-s3": "3.142.0",
    "@aws-sdk/lib-dynamodb": "3.142.0",
    "@aws-sdk/lib-storage": "3.142.0",
    "@aws-sdk/signature-v4-crt": "3.130.0",
    "@aws-sdk/smithy-client": "3.142.0",
    "@fastify/cookie": "7.3.1",
    "agentkeepalive": "4.2.1",
    "aws4-axios": "2.4.9",
    "axios": "0.27.2",
    "cheerio": "1.0.0-rc.10",
    "fastify": "4.3.0",
    "handlebars": "4.7.7",
    "jsonwebtoken": "8.5.1",
    "jwk-to-pem": "2.0.5",
    "nanoid": "3.3.4"
  },
  "devDependencies": {
    "@aws-sdk/types": "3.127.0",
    "@types/eslint": "8.4.5",
    "@types/jsonwebtoken": "8.5.8",
    "@types/jwk-to-pem": "2.0.1",
    "@types/node": "^16.0.0",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "clean-webpack-plugin": "4.0.0",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsdoc": "39.3.4",
    "nodemon-webpack-plugin": "4.8.1",
    "prettier": "2.7.1",
    "ts-loader": "9.3.1",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-node-externals": "3.0.0"
  }
}
